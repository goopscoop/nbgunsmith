<section class="well well2 {{#if background}}{{background}}{{^}}bg1{{/if}}">
  <div class="container">
  <h2>
    {{title.bold}}
    <small>
      {{title.small}}
    </small>

  </h2>
    <div class="row offs1">
      <div class="col-md-6 col-sm-12">
        <ul class="link-list wow fadeInLeft" data-wow-duration='2s'>
          {{#each items}}
          <li class="service-list">
            <a href="{{link}}">
              {{item}}
            </a>
            <a href="#" class="btn-link l-h1 fa-angle-right"></a>
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="col-md-6 col-sm-12">
        {{#if imagepath}}<img class="width_img" src="{{imagepath}}" alt="">{{/if}}
        {{#if description}}{{{description}}}{{/if}}
      </div>
    </div>
  </div>
</section>